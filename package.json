{
  "name": "@sb-mig/plugin-create-component",
  "description": "Plugin for sb-mig to create component in monorepos compliant with sb-mig",
  "version": "0.0.5",
  "author": "Marcin Krawczyk marckraw@icloud.com",
  "bugs": "https://github.com/sb-mig/plugin-create-component/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "execa": "^4.0.0",
    "ora": "^4.0.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/sb-mig/plugin-create-component",
  "keywords": [
    "create-component",
    "sb-mig",
    "storyblok",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sb-mig",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sb-mig/plugin-create-component",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s && oclif-dev readme && git add README.md && git add CHANGELOG.md"
  }
}
